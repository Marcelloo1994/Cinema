<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="create-table-cinema-0001" author="marcelloCescutti">
        <comment>Create table Cinema</comment>

        <createTable tableName="cinema">
            <column name="id" type="INT">
                <constraints primaryKey="true" primaryKeyName="pkCinema"/>
            </column>
        </createTable>
        <rollback>
            <dropTable tableName="cinema"/>
        </rollback>

    </changeSet>
</databaseChangeLog>